ppublic class Intnode
{
    private int _value;
    private Intnode _next;
    
    public Intnode(int value)
    {
        _value = value;
        _next = null;
    }
    
    public Intnode(int value, Intnode next)
    {
        _value = value;
        _next = next;
    }
    
    public int get_value() { return _value; }
    
    public Intnode get_next() { return _next; }
    
    public void set_value(int value) { _value = value; }
    
    public void set_next(Intnode next) { _next = next; }
}
